    "highlight.js": "^11.8.0",
    "highlightjs-svelte": "^1.0.6",
    "svelte": "^3.54.0",
    "svelte-fa": "^3.0.4",
    "svelte-file-dropzone": "^2.0.1",
    "svelte-headless-table": "^0.17.3",
    "svelte-select": "^5.6.0",
    "vest": "^4.6.11"
  },
  "author": "David Sch√∂ne",
